		fi
	fi
}

IMAGE_CMD_f2fs() {
        # We need to add additional smarts here form devices smaller than 1.5G
        # Need to scale appropriately between 40M -> 1.5G as the "overprovision
        # ratio" goes down as the device gets bigger (70% -> 4.5%), below about
        # 500M the standard IMAGE_OVERHEAD_FACTOR does not work, so add additional
        # space here when under 500M
	size=${ROOTFS_SIZE}
	if [ ${size} -lt 524288 ] ; then
		size=524288
		bbwarn "Rootfs size is too small for F2FS. Filesystem will be extended to ${size}K"
	fi
	dd if=/dev/zero of=/home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/deploy-core-image-ik-x11-image-complete/core-image-ik-x11-qemux86-64-20190929114145.rootfs.f2fs seek=${size} count=0 bs=1024
	mkfs.f2fs  /home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/deploy-core-image-ik-x11-image-complete/core-image-ik-x11-qemux86-64-20190929114145.rootfs.f2fs
	sload.f2fs -f /home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/rootfs /home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/deploy-core-image-ik-x11-image-complete/core-image-ik-x11-qemux86-64-20190929114145.rootfs.f2fs
