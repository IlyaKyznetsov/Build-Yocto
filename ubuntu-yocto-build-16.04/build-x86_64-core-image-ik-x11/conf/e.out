linuxloader() {
	linuxloader_glibc
}

linuxloader_glibc() {
	case x86_64 in
		powerpc | microblaze )
			dynamic_loader="/lib/ld.so.1"
			;;
		mipsisa32r6el | mipsisa32r6 | mipsisa64r6el | mipsisa64r6)
			dynamic_loader="/lib/ld-linux-mipsn8.so.1"
			;;
		mips* )
			dynamic_loader="/lib/ld.so.1"
			;;
		powerpc64)
			dynamic_loader="/lib/ld64.so.1"
			;;
		x86_64)
			dynamic_loader="/lib/ld-linux-x86-64.so.2"
			;;
		i*86 )
			dynamic_loader="/lib/ld-linux.so.2"
			;;
		arm )
			dynamic_loader="/lib/ld-linux.so.3"
			;;
		* )
			dynamic_loader="/unknown_dynamic_linker"
			;;
	esac
	echo $dynamic_loader
}

linuxloader_musl() {
	case x86_64 in
		microblaze* )
			dynamic_loader="/lib/ld-musl-microblazeel.so.1"
			;;
		mips* )
			dynamic_loader="/lib/ld-musl-mips${MIPSPKGSFX_BYTE}${MIPSPKGSFX_R6}${MIPSPKGSFX_ENDIAN}.so.1"
			;;
		powerpc )
			dynamic_loader="/lib/ld-musl-powerpc.so.1"
			;;
		powerpc64 )
			dynamic_loader="/lib/ld-musl-powerpc64.so.1"
			;;
		x86_64 )
			dynamic_loader="/lib/ld-musl-x86_64.so.1"
			;;
		i*86 )
			dynamic_loader="/lib/ld-musl-i386.so.1"
			;;
		arm* )
			dynamic_loader="/lib/ld-musl-arm${ARMPKGSFX_ENDIAN}${ARMPKGSFX_EABI}.so.1"
			;;
		aarch64* )
			dynamic_loader="/lib/ld-musl-aarch64${ARMPKGSFX_ENDIAN_64}.so.1"
			;;
		* )
			dynamic_loader="/unknown_dynamic_linker"
			;;
	esac
	echo $dynamic_loader
}

