prelink_image() {
#	export PSEUDO_DEBUG=4
#	/bin/env | /bin/grep PSEUDO
#	echo "LD_LIBRARY_PATH=$LD_LIBRARY_PATH"
#	echo "LD_PRELOAD=$LD_PRELOAD"

	pre_prelink_size=`du -ks /home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/rootfs | awk '{size = $1 ; print size }'`
	echo "Size before prelinking $pre_prelink_size."

	# We need a prelink conf on the filesystem, add one if it's missing
	if [ ! -e /home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/rootfs/etc/prelink.conf ]; then
