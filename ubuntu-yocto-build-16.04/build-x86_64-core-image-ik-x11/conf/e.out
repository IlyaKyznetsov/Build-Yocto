			/home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/rootfs/etc/prelink.conf
		dummy_prelink_conf=true;
	else
		dummy_prelink_conf=false;
	fi

	# We need a ld.so.conf with pathnames in,prelink conf on the filesystem, add one if it's missing
	ldsoconf=/home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/rootfs/etc/ld.so.conf
	if [ -e $ldsoconf ]; then
		cp $ldsoconf $ldsoconf.prelink
	fi
	cat /home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/recipe-sysroot/etc/ld.so.conf >> $ldsoconf

	dynamic_loader=$(linuxloader)

	# prelink!
	if [ "${BUILD_REPRODUCIBLE_BINARIES}" = "1" ]; then
		bbnote " prelink: BUILD_REPRODUCIBLE_BINARIES..."
		if [ "$REPRODUCIBLE_TIMESTAMP_ROOTFS" = "" ]; then
			export PRELINK_TIMESTAMP=`git log -1 --pretty=%ct `
		else
			export PRELINK_TIMESTAMP=$REPRODUCIBLE_TIMESTAMP_ROOTFS
		fi
		/home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/recipe-sysroot-native/usr/sbin/prelink --root /home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/rootfs -am -N -c /etc/prelink.conf --dynamic-linker $dynamic_loader
	else
		/home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/recipe-sysroot-native/usr/sbin/prelink --root /home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/rootfs -amR -N -c /etc/prelink.conf --dynamic-linker $dynamic_loader
	fi

	# Remove the prelink.conf if we had to add it.
	if [ "$dummy_prelink_conf" = "true" ]; then
		rm -f /home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/rootfs/etc/prelink.conf
	fi

	if [ -e $ldsoconf.prelink ]; then
		mv $ldsoconf.prelink $ldsoconf
	else
		rm $ldsoconf
	fi

	pre_prelink_size=`du -ks /home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/rootfs | awk '{size = $1 ; print size }'`
	echo "Size after prelinking $pre_prelink_size."
}

read_only_rootfs_hook() {
	# Tweak the mount option and fs_passno for rootfs in fstab
	if [ -f /home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/rootfs/etc/fstab ]; then
		sed -i -e '/^[#[:space:]]*\/dev\/root/{s/defaults/ro/;s/\([[:space:]]*[[:digit:]]\)\([[:space:]]*\)[[:digit:]]$/\1\20/}' /home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/rootfs/etc/fstab
	fi

	# If we're using openssh and the /etc/ssh directory has no pre-generated keys,
	# we should configure openssh to use the configuration file /etc/ssh/sshd_config_readonly
	# and the keys under /var/run/ssh.
	if [ -d /home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/rootfs/etc/ssh ]; then
		if [ -e /home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/rootfs/etc/ssh/ssh_host_rsa_key ]; then
			echo "SYSCONFDIR=\${SYSCONFDIR:-/etc/ssh}" >> /home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/rootfs/etc/default/ssh
			echo "SSHD_OPTS=" >> /home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/rootfs/etc/default/ssh
		else
			echo "SYSCONFDIR=\${SYSCONFDIR:-/var/run/ssh}" >> /home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/rootfs/etc/default/ssh
			echo "SSHD_OPTS='-f /etc/ssh/sshd_config_readonly'" >> /home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/rootfs/etc/default/ssh
		fi
	fi

	# Also tweak the key location for dropbear in the same way.
	if [ -d /home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/rootfs/etc/dropbear ]; then
		if [ ! -e /home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/rootfs/etc/dropbear/dropbear_rsa_host_key ]; then
			echo "DROPBEAR_RSAKEY_DIR=/var/lib/dropbear" >> /home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/rootfs/etc/default/dropbear
		fi
	fi

	if false; then
		# Change the value of ROOTFS_READ_ONLY in /etc/default/rcS to yes
		if [ -e /home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/rootfs/etc/default/rcS ]; then
			sed -i 's/ROOTFS_READ_ONLY=no/ROOTFS_READ_ONLY=yes/' /home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/rootfs/etc/default/rcS
		fi
		# Run populate-volatile.sh at rootfs time to set up basic files
		# and directories to support read-only rootfs.
		if [ -x /home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/rootfs/etc/init.d/populate-volatile.sh ]; then
			/home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/rootfs/etc/init.d/populate-volatile.sh
		fi
	fi
}

