IMAGE_CMD_cpio() {
	(cd /home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/rootfs && find . | cpio -o -H newc >/home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/deploy-core-image-ik-x11-image-complete/core-image-ik-x11-qemux86-64-20190929114145.rootfs.cpio)
	# We only need the /init symlink if we're building the real
	# image. The -dbg image doesn't need it! By being clever
	# about this we also avoid 'touch' below failing, as it
	# might be trying to touch /sbin/init on the host since both
	# the normal and the -dbg image share the same WORKDIR
	if [ "${IMAGE_BUILDING_DEBUGFS}" != "true" ]; then
		if [ ! -L /home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/rootfs/init ] && [ ! -e /home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/rootfs/init ]; then
			if [ -L /home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/rootfs/sbin/init ] || [ -e /home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/rootfs/sbin/init ]; then
				ln -sf /sbin/init /home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/cpio_append/init
			else
				touch /home/build/build-x86_64-core-image-ik-x11/tmp/work/qemux86_64-poky-linux/core-image-ik-x11/1.0-r0/cpio_append/init
			fi
